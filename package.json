{
  "name": "api-session-manager-sdk",
  "version": "0.0.21",
  "description": "Client SDK generator for session manager REST service",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "sdk:generate": "SYNC_SERVICES='api-session-manager' GITHUB_TOKEN=$(cat ~/.github_token) node .scripts/sync.js && node .scripts/generator.js",
    "release": "yarn sdk:generate && yarn build && .scripts/release.sh"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "dotenv": "^16.0.0",
    "typescript": "^4.5.2"
  },
  "devDependencies": {
    "swagger-typescript-api": "^9.3.1"
  }
}
